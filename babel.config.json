{
  "$schema": "https://json.schemastore.org/babelrc.json",
  "presets": [
    [
      "@babel/preset-env"
    ]
  ],
  "plugins": [
    "babel-plugin-transform-globalthis"
  ],
  "compact": true,
  "transformIgnorePatterns": [
    "node_modules/(?!(data-uri-to-buffer|formdata-polyfill|fetch-blob|node-fetch)/)"
  ]
}
